{
  "version": 2,
  "builds": [
    {
      "src": "index.js", // Replace with the entry point of your server file
      "use": "@vercel/node" // Specify the Vercel builder to use
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js" // Replace with the entry point of your server file
    }
  ]
}
