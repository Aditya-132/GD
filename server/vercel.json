{
  "version": 2,
  "builds": [
    {
      "src": "index.js", // Replace with the entry point of your server file
      "use": "@vercel/node" // Specify the Vercel builder to use
    }
  ],
  "routes": [
    { "handle": "filesystem" }, // Handle static assets
    { "src": "/.*", "dest": "index.js" } // Route all other requests to the server
  ]
}
